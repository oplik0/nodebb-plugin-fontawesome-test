<div class="row" id="fontawesome-test">
    <div class="row">
        <h2>Normal class reference</h2>
        {{{ each classes }}}
        <div class="col-sm-1">
            <i class="fa { @value }"></i>
        </div>
        {{{ end }}}
    </div>
    <div class="row">
        <h2>Direct value in HTML (font in CSS)</h2>
        {{{ each unicode }}}
        <div class="col-sm-1">
            <i class="fa-old-font">{ @value }</i>
        </div>
        {{{ end }}}
    </div>
    <div class="row">
        <h2>SCSS variables</h2>
        {{{ each customClasses }}}
        <div class="col-sm-1">
            <i class="{ @value }"></i>
        </div>
        {{{ end }}}
    </div>
    <div class="row">
        <h2>Modifier: { classModifiers.0 }</h2>
