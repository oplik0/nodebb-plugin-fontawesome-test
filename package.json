{
    "name": "nodebb-plugin-fontawesome-test",
    "version": "0.5.0",
    "description": "A starter kit for quickly creating NodeBB plugins",
    "main": "library.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/psychobunny/nodebb-plugin-fontawesome-test"
    },
    "keywords": [
        "nodebb",
        "plugin",
        "fontawesome-test",
        "shell"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "author": {
        "name": "psychobunny",
        "email": "psycho.bunny@hotmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/psychobunny/nodebb-plugin-fontawesome-test/issues"
    },
    "readmeFilename": "README.md",
    "nbbpm": {
        "compatibility": "^3.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "13.1.0",
        "@commitlint/config-angular": "13.1.0",
        "eslint": "9.2.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-plugin-import": "2.23.4",
        "husky": "7.0.1",
        "lint-staged": "11.1.1"
    }
}